{
    "name": "webshop/app",
    "description": "Modern PHP Webshop with Admin Panel",
    "type": "project",
    "require": {
        "php": "^8.1",
        "bramus/router": "^1.6",
        "twig/twig": "^3.0",
        "vlucas/phpdotenv": "^5.4"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    }
}
